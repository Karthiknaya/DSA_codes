class Solution {
    public int heightChecker(int[] heights) {
        int n =heights.length;
        int[] arr=new int[n];
        for(int i=0;i<heights.length;i++)
        {
            arr[i]=heights[i];
        }
        Arrays.sort(heights);
        int ans=0;
        for(int i=0;i<n;i++)
        {
            if(arr[i]!=heights[i])
            {
                ans++;
            }
        }
        return ans;
    }
}
