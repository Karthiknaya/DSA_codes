class Solution {
    public List<List<Integer>> generate(int numRows) {
       List<List<Integer>> pascalfinal=new ArrayList<>();
       List<Integer> firstrow=new  ArrayList<>();
       firstrow.add(1);
       pascalfinal.add(firstrow);
       for(int i=1;i<numRows;i++)
       {
        List<Integer> prev=pascalfinal.get(i-1);
        List<Integer> row=new ArrayList<>();
        row.add(1);
        for(int j=1;j<i;j++)
        {
            row.add(prev.get(j-1)+prev.get(j));
        }
        row.add(1);
        pascalfinal.add(row);
       }
       return pascalfinal;
    }
}
